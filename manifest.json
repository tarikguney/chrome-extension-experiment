{
  "name": "Eat Your Food",
  "description": "Helps parents have their kids finish their plates when watching animations.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "bookmarks"
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["bookmark-listener-content-script.js"]
    }
  ],
  "background": {
    "service_worker": "bookmark-explorer.js"
  },
  "icons": {
    "16": "plum16.png",
    "48": "plum64.png",
    "128": "plum128.png"
  },
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  }
}